{
	"description": "Grammar for clinical cases",
	"unionid": "Patient/id",
	"grammarname": "Clinical Case",
	"structure": [
		{
			"name": "Patient Data",
			"sons": [
				{
					"name": "Patient Description",
					"eq": "Patient/text/div"
				},
				{
					"name": "Name",
					"eq": [
						"Patient/name/entry/given/entry",
						"Patient/name/entry/given/family"
					]
				},
				{
					"name": "Birth Date",
					"eq": "Patient/birthDate"
				},
				{
					"name": "Gender",
					"eq": "Patient/gender"
				}
			]
		},
		{
			"name": "Diagnostic Data",
			"sons": [
				{
					"name": "Observation",
					"eq": "Observation",
					"grammar": "Observation",
					"sons": [
					{
							"name": "Description",
							"eq": "Observation/text/div"
							
						},
						{
							"name": "LOINC",
							"eq": "Observation/code/coding/entry",
							"multitype": "elem",
							"sons": [
								{
									"name": "Value",
									"eq": [
										"Observation/code/coding/entry/display",
										"Observation/code/coding/entry/unit"
									]
								}
							]
						},
						{
							"name": "Observation Date",
							"eq": "Observation/effectiveDateTime"
						},
						{
							"name": "Category",
							"multitype": "elem",
							"eq": "Observation/category/entry/",
							"sons": [
								{
									"name": "Codigo",
									"eq": "Observation/category/entry/coding/entry/display"
								}
							]
						},
						{
							"name": "Range",
							"sons":[{
							"name": "Low",
							"eq": ["Observation/valueRange/low/value", "Observation/valueRange/low/code"]
							},{
							"name": "Hight",
							"eq": ["Observation/valueRange/hight/value", "Observation/valueRange/hight/code"]
							}]
							
						},
						{
							"name": "Measure 1",
							"eq": ["Observation/valueQuantity/value", "Observation/valueQuantity/code"]
	
						},
						{
							"name": "Measure 2",
							"multitype": "elem",
							"eq": "Observation/component/entry",
							"sons":[
							{
							"name": "Code",
							"eq": ["Observation/component/entry/code/coding/entry/code", "Observation/component/entry/code/coding/entry/display"]
							
							},
							{
							"name": "Value",
							"eq": ["Observation/component/entry/valueQuantity/value","Observation/component/entry/valueQuantity/unit"]
							}
							]
	
						}
					]
				}
			]
		},
		{
			"name": "Images",
			"eq": "Endpoint/address",
			"grammar": "Endpoint",
			"sons": [
				{
					"name": "Description",
					"eq": "Endpoint/name"
				},
				{
					"name": "Image",
					"eq": "Endpoint/address"
				}
			]
		},
		{
			"name": "Conditions",
			"eq": "SNOMED",
			"grammar": "SNOMED",
			"sons": [
				{
					"name": "Condition Value",
					"eq": "SNOMED/term"
				}
			]
		}
		
	]
}
